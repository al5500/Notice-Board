<h2>Notice Board</h2>
<h3><%= link_to('New Notice', '/notices/new') %></h3>
<ol>
  <% @notices.each do |notice| %>
    <li><div class="notice_heading">Notice: <%= notice.heading %><br></div>
    User: <%= notice.user.name %> | Category: <%= notice.category.name %> | Time: <%= notice.created_at %><br>
     <div class="notice_message">Details: <%= notice.message %><br></div>
     
     
    </li>
  <% end %>
</ol>